mponents/userProfile-form/index.js","original":{"line":3,"column":30}},{"name":"Component","generated":{"line":105,"column":17},"source":"components/userProfile-form/index.js","original":{"line":3,"column":36}},{"name":"UserProfileForm","generated":{"line":107,"column":15},"source":"components/userProfile-form/index.js","original":{"line":51,"column":15}}],"sources":{"components/userProfile-form/index.js":"import React from 'react';\n\nclass UserProfileForm extends React.Component {\n  constructor(props){\n    super(props)\n    this.state = props.userProfile ? {...props.userProfile} :{\n      country: '',\n      image: '',\n    };\n  }\n  componentWillReceiveProps(props) {\n    if (props.userProfile) this.setState(props.userProfile);\n  }\n  handleSubmit = e => {\n    e.preventDefault();\n    return this.props.onComplete(this.state);\n  };\n  handleChange = e => {\n    this.setState({\n      [e.target.name]: e.target.value\n    })\n  };\n  render() {\n    return (\n      <section className='profile-form'>\n        <form className='form' onSubmit={this.handleSubmit}>\n          <h2 className='title'>{this.props.profileAction} your profile.</h2>\n          <label htmlFor='country' className='profileFormLabel'>Country: </label>\n          <input \n            type=\"text\" \n            placeholder=\"Country\"\n            value={this.state.country || ''}\n            name=\"country\"\n            onChange={this.handleChange}/>\n          <label htmlFor='image' className='profileFormLabel'>Profile img URL: </label>\n          <input \n            type=\"text\" \n            placeholder=\"img url\"\n            value={this.state.image || ''}\n            name=\"image\"\n            onChange={this.handleChange}/>\n          <div className='buttonAuthFormParentDiv'>\n            <button className='formButton' type=\"submit\">Submit</button>\n          </div>\n        </form>  \n      </section>\n    );\n  }    \n}  \n\nexport default UserProfileForm;"},"lineCount":null}},